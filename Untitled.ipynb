   "source": [
    "import numpy as np\n",
    "import pandas as pd\n",
    "dt = 1/10\n",
    "freq = 1.5\n",
    "omega = 2*np.pi*freq\n",
    "t = np.arange(0,10,dt)\n",
    "y = np.sin(omega*t)\n",
    "z = np.cos(omega*t)\n",
    "df = pd.DataFrame({'Time':t, 'YVals':y, 'ZVals':z})\n",
    "print(df.head())\n",
    "outfile = 'out.txt'\n",
    "df[10:16][['YVals', 'ZVals']].to_csv(outfile)\n",
    "\n"
   ]
